{
  "name": "oem3",
  "module": "index.ts",
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.6.0"
  },
  "peerDependencies": {
    "typescript": "^5.4.2"
  },
  "scripts": {
    "dev": "cd server && bun run dev",
    "build": "docker build .",
    "start": "docker compose up",
    "preview": "docker compose up --build",
    "count-lines": "bunx cloc . --exclude-dir=node_modules,build,.svelte-kit,.git,drizzle,types --exclude-ext=lock,json",
    "count-lines:svelte": "bunx cloc ./svelte --exclude-dir=node_modules,build,.svelte-kit --exclude-ext=lock,json",
    "count-lines:server": "bunx cloc ./server --exclude-dir=node_modules,types,drizzle --exclude-ext=lock,json"
  },
  "type": "module",
  "dependencies": {}
}